{
  "name": "skupper-x-webconsole",
  "version": "1.0.0",
  "description": "Skupper X prototype",
  "license": "Apache-2.0",
  "keywords": [
    "skupper",
    "skupper-x",
    "console",
    "monitoring",
    "observability",
    "connectivity",
    "openshift"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skupperproject/skupper-console.git"
  },
  "bugs": {
    "url": "https://github.com/skupperproject/skupper-console/issues"
  },
  "private": true,
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "ENABLE_MOCK_SERVER=true jest --config jest.config.ts",
    "coverage": "yarn test --coverage --collectCoverageFrom='src/**/*.{tsx,ts}'",
    "lint": "eslint src --ext .ts,.tsx --cache",
    "lint-fix": "yarn lint --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,json,css}'",
    "bundle-report": "STATS=server yarn build",
    "find-deadcode": "ts-prune",
    "prepare": "husky",
    "commit": "git-cz"
  },
  "dependencies": {
    "@antv/g6": "^4.8.25",
    "@patternfly/patternfly": "^5.4.2",
    "@patternfly/react-charts": "^7.4.9",
    "@patternfly/react-code-editor": "^5.4.18",
    "@patternfly/react-core": "^5.4.14",
    "@patternfly/react-icons": "^5.4.2",
    "@patternfly/react-table": "^5.4.16",
    "@tanstack/react-query": "^5.90.21",
    "axios": "^1.13.5",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.18.2",
    "node": "^21.7.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-router-dom": "^6.30.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "circular-dependency-plugin": "^5.2.2",
    "commitizen": "^4.3.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.11.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "cz-git": "^1.12.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.6",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.10.0",
    "miragejs": "^0.1.48",
    "prettier": "^3.8.1",
    "start-server-and-test": "^2.1.5",
    "style-loader": "^3.3.4",
    "terser-webpack-plugin": "^5.3.16",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.3",
    "webpack": "^5.105.3",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.3",
    "webpack-merge": "^5.10.0"
  },
  "engines": {
    "node": ">=18.17.1",
    "yarn": ">=1.22.10"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "browserslist": [
    ">10%",
    "last 2 versions",
    "not ie <= 11"
  ]
}
